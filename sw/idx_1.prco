
__stdcall fnc main()
{
    int GPIO0  = 0x90;
    int GPIO1  = 0x91;
    int GPIO2  = 0x92;
    int UART0  = 0xA0;
    int IDX    = 0x80;
    int CORES  = 0xB0;
    int SLAVES = 0xB1;

    int start;

    if ((@IDX) EQ 1) {
        100;
    } else {
        200;
    }

    start = (@IDX) * 5;

    $(UART0) = start + 48;

    50;
}
